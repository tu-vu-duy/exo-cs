<%
	import org.exoplatform.contact.webui.UIContactPreview;
	import org.exoplatform.contact.webui.UIContacts;
	String compId = uicomponent.getId() ;
%>

<div class="UIContactContainer" id="$compId">
<!--  Start UIContacts-->
	<div class="UIMessageArea SpliterResizableListArea" id="uiMessageListResizableArea">
	<% uicomponent.renderChild(UIContacts.class) ;%>		
	</div>	
<!--  End UIContacts -->
	<div class="ResizeReadingPane" onmousedown = "eXo.cs.Spliter.doResize(event, this, document.getElementById('UIListUsers').parentNode, 'UIContactPreview') ;"><span></span></div>
		
<!--  Start UIContactPreview-->
	<% uicomponent.renderChild(UIContactPreview.class) ;%>	
<!--  End UIContactPreview -->		
		
</div>