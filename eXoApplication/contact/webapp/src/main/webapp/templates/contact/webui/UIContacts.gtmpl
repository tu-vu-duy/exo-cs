										String actionLink  = uicomponent.event("SelectedContact", contact.getId()) ;
										checkboxField = uicomponent.getChildById(contact.getId()) ;
										iconTag = null ;
										String[] tagIds = contact.getTags() ;
										StringBuffer tagsName = new StringBuffer() ;
										if (tagIds != null && tagIds.length > 0) {
											Tag tag = tagMap.get(tagIds[0]) ;
											boolean hasComma = false ;
											if (tag != null) {
												tagsName.append(tag.getName()) ;
												hasComma = true ;
												iconTag = "Icon " + tag.getColor() + "Tag" ;
											} else { hasComma = false ; }
											i = 1 ;
											while (i < tagIds.length) {
												tag = tagMap.get(tagIds[i]) ;
												if (tag != null && hasComma) {
													tagsName.append(", " + tag.getName()) ;
													if (ContactUtils.isEmpty(iconTag)) iconTag = "Icon " + tag.getColor() + "Tag" ;
												}
												else if (tag != null) {
														tagsName.append(tag.getName()) ;
														hasComma = true ;
														if (ContactUtils.isEmpty(iconTag)) iconTag = "Icon " + tag.getColor() + "Tag" ;
												}
												i ++ ;
											}		
										}	
										cssClass = "" ;
										if (contact.getId().equals(uicomponent.getSelectedContact())) cssClass = "Selected" ;								
						%>
							
  						<tr class="UIContactList $cssClass" isSearch="<%=uicomponent.isDisplaySearchResult()%>" isPublic="<%=contact.isOwner()%>">
  						  <td class="text" style="padding-left: 7px;">
  						  	<%
  						  		if(checkboxField != null) uicomponent.renderField(checkboxField) ;
