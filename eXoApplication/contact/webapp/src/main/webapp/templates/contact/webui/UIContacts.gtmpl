		  	 	    			while (i < addressBooks.length) {
		  	 	    				if(buffer.length() > 0) buffer.append(", ").append(addressBooks[i]) ;
		  	 	    				else buffer.append(addressBooks[i]) ;
		  	 	    				i ++ ;
