    if (fromIndex == endIndex)
    	contactRange = _ctx.appRes(uicomponent.getName() + ".label.contacts")+ "&nbsp;" + fromIndex +  "&nbsp;" + _ctx.appRes(uicomponent.getName() + ".label.of") + "&nbsp;" + availableContact ;
