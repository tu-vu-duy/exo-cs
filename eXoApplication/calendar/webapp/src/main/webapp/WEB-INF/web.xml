<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
 PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
  "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>calendar</display-name>

	<filter>
    <filter-name>ResourceRequestFilter</filter-name>
    <filter-class>org.exoplatform.portal.application.ResourceRequestFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ResourceRequestFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <!-- remove this servlet when finish implement rss by webservice -->
  <servlet>
    <servlet-name>icalservlet</servlet-name>
    <servlet-class>org.exoplatform.calendar.ICalServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>icalservlet</servlet-name>
    <url-pattern>/iCalRss/*</url-pattern>
  </servlet-mapping> 

</web-app>
